if (body) {
  let data = JSON.parse($response.body);
  const subscription = {
    status: "intro-trial",
    expiration_ts: "2222-10-18T02:52:03+00:00",
    trial_available: true,
    on_hold: false,
    trial_duration: "P14D"
  }
  data.subscription = subscription;
  $done({body: JSON.stringify(data)});
} else {
  $done({});
}
